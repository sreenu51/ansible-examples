[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = A few starter examples of ansible playbooks, to show features and how they work together.  See http://galaxy.ansible.com for example roles from the Ansible community for deploying many popular applications.
[access "refs/*"]
	owner = group user/sreenu51
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/sreenu51
	label-Verified = -1..+1 group user/sreenu51
	push = group user/sreenu51
	read = group user/sreenu51
	rebase = group user/sreenu51
	removeReviewer = group user/sreenu51
	submit = group user/sreenu51
[access "refs/heads/*"]
	create = group user/sreenu51
	push = +force group user/sreenu51
	pushMerge = group user/sreenu51
	read = group user/sreenu51
[access "refs/tags/*"]
	push = group user/sreenu51
	push = +force group user/sreenu51
